BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//HUGO//gohugo.io//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{{- range .Pages -}}
{{- $dateTime := (time (print .Params.EventDate (string "T") .Params.EventTime) .Params.TimeZone).UTC -}}
{{- $duration := 3600 -}}
{{- if .Params.DurationMinutes }}{{ $duration := mul .Params.DurationMinutes 60 }}{{ end -}}
{{- $endTime := add $dateTime.Unix $duration | time.AsTime -}}
{{- $authorDate := .GitInfo.AuthorDate.UTC -}}
{{- $lastModified := .Lastmod.UTC }}
BEGIN:VEVENT
UID: {{ .Permalink | md5 }}@sighpceducation.acm.org
STATUS: CONFIRMED
SEQUENCE:0
DTSTAMP:{{ $authorDate.Format "20060102T150405"}}
{{ if .Params.AllDay }}DTSTART:{{ $dateTime.Format "20060102" }}
{{else}}DTSTART:{{ $dateTime.Format "20060102T150405" }}
DTEND:{{ $endTime.Format "20060102T150405" }}{{end}}
SUMMARY:{{ .Title }} - {{ .Parent.Parent.Title }} - {{ .Site.Title }}
{{- if .Params.Location }}LOCATION:{{ .Params.Location }}{{ end }}
DESCRIPTION:{{ .Summary }}
CLASS:PUBLIC
URL:{{ .Permalink }}
END:VEVENT
{{- end }}
END:VCALENDAR