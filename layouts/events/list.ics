BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//HUGO//gohugo.io//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH

{{ range .Pages }}
{{ $dateTime := time (print .Params.EventDate (string "T") .Params.EventTime) .Params.TimeZone }}

BEGIN:VEVENT
UID: {{ .Permalink | md5 }}@sighpceducation.acm.org
STATUS: CONFIRMED
SEQUENCE:0
DTSTAMP:{{ .GitInfo.AuthorDate }}
CREATED:{{ .GitInfo.AuthorDate }}
LAST-MODIFIED:{{ .Lastmod }}
DTSTART;VALUE=DATE|TZID=: 
SUMMARY:{{ .Title }} - {{ .Parent.Title }} - {{ .Site.Title }}
{{ if .Params.Location }}LOCATION:{{ .Params.Location }}{{ end }}
DESCRIPTION:{{ .Summary }}
CLASS:PUBLIC
URL:{{ .Permalink }}
END:VEVENT
{{ end }}

END:VCALENDAR